			hactive = <720>;
			vactive = <1280>;
			hfront-porch = <80>;
			hsync-len = <10>;
			hback-porch = <150>;
			vfront-porch = <20>;
			vsync-len = <10>;
			vback-porch = <20>;
			hsync-active = <0>;
			vsync-active = <0>;
			de-active = <0>;
		};
	};
};

&panel0 {
	status = "disabled";

	display-timings {

		timing0: timing {
			/* 720x480@60 */
			clock-frequency = <27000000>;
			hactive = <720>;
			vactive = <480>;
			hfront-porch = <16>;
			hsync-len = <62>;
			hback-porch = <60>;
			vfront-porch = <19>;
			vsync-len = <6>;
			vback-porch = <30>;
			hsync-active = <1>;
			vsync-active = <1>;
			de-active = <1>;
		};
	};
};

&ax_perf_bm {
       status = "okay";
};

&hwlock {
       status = "okay";
};

&vdsp {
	memory-region = <&vdsp0_reserved>, <&vdsp1_reserved>;
	status = "disabled";
};

&eth0 {
	status = "disabled";

	phy-handle = <&phy0>;
	phy-mode = "gmii";

    mdio {
        #address-cells = <1>;
        #size-cells = <0>;
        compatible = "snps,dwmac-mdio";
        phy0: phy@0 {
            device_tpye = "ethernet-phy";
            compatible = "marvell,88E1111", "ethernet-phy-ieee802.3-c22";
            reg = <0>;
        };
    };
};

&eth1 {
	status = "okay";

	phy-handle = <&phy1>;
	phy-mode = "gmii";

    mdio {
        #address-cells = <1>;
        #size-cells = <0>;
        compatible = "snps,dwmac-mdio";
        phy1: phy@0 {
            device_tpye = "ethernet-phy";
            compatible = "marvell,88E1111", "ethernet-phy-ieee802.3-c22";
            reg = <0>;
        };
    };
};
