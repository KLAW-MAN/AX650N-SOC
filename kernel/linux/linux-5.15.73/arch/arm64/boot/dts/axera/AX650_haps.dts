/dts-v1/;
#include "AX650.dtsi"
/ {
	memory@100000000 {
		device_type = "memory";
		reg = <0x1 0x00000000 0x1 0x00000000>;
	};
	reserved-memory {
		#address-cells = <2>;
		#size-cells = <2>;
		ranges;
		linux,cma {
			compatible = "shared-dma-pool";
			reusable;
			size = <0x00000000 0x10000000>;
			linux,cma-default;
		};
		ramoops_mem@106000000 {
			compatible = "ramoops";
			reg = <0x4 0x06000000 0x0 0xf0000>;
			record-size = <0x40000>;
			console-size = <0x80000>;
			ftrace-size = <0x20000>;
		};
		e907_dram: riscv_memserved {
			reg = <0x01 0x36000000 0x0 0x400000>;
			no-map;
		};
		riscv_log_mem: riscv_log_memserved {
			reg = <0x01 0x36400000 0x0 0x1000>;
			no-map;
		};
		vdsp0_reserved: vdsp0_reserved@13E000000 {
			no-map;
			reg = <0x1 0x3E000000 0x0 0x1000000>;
		};
		vdsp1_reserved: vdsp1_reserved@13F000000 {
			no-map;
			reg = <0x1 0x3F000000 0x0 0x1000000>;
		};
	};
	osal_dummy@0 {
		compatible = "axera,osal_dummy";
		status = "okay";
	};

	chosen {
		bootargs = "mem=2048M rdinit=/linuxrc console=ttyS0,230400n8 loglevel=4";
	};

